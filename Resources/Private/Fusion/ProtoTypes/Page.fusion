prototype(Neos.Neos:Page) {

    head {

        flickrphotogalleryCss = Neos.Fusion:Tag {
            @position = 'before stylesheets'
            tagName = 'link'
            attributes {
                media = 'all'
                rel = 'stylesheet'
                href = Neos.Fusion:ResourceUri {
                    path = 'resource://Flickr.Photogallery/Public/Styles/flickr-photogallery.css'
                }
            }
        }

    }

}
